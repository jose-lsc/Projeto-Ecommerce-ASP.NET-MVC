@model UserModel

<form asp-controller="Login" asp-action="Logon" method="post">
    <div>
        <label>Login:</label>
        <input type="text" name="login" required  autocomplete="off"/>
    </div>
    <div>
        <label>Senha:</label>
        <input type="password" name="senha" required autocomplete="off" />
    </div>
    <button type="submit">Entrar</button>
</form>
<a class="nav-link text-dark" asp-area="" asp-controller="User" asp-action="Cadastrar">Cadastrar</a>


